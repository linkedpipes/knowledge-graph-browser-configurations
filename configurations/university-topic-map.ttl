<https://linked.opendata.cz/resource/knowledge-graph-browser/detail-query/uk/témata/téma> a browser:DetailQuery ;
  dct:title "Základní detail tématu" ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/uk/pracoviště> ;
  browser:query """PREFIX dct: <http://purl.org/dc/terms/>
PREFIX schema: <http://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX buk: <https://linked.opendata.cz/ontology/knowledge-graph-browser/uk/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>

CONSTRUCT {
  ?node dct:title ?label ;
    buk:počet-pracovišť ?početPracovišť ;
    buk:počet-předmětů ?početPředmětů ;
    ?pFakulta ?názvyPředmětů .
} WHERE {
  ?node dct:title ?label .
  {
    SELECT ?node ?pFakulta (GROUP_CONCAT(DISTINCT ?labelPředmětu ; separator=" - " ) AS ?názvyPředmětů)
    WHERE {
      ?předmět schema:provider ?pracoviště ;
        dct:title ?název ;
        schema:courseCode ?code ;
        schema:keywords ?node .
      FILTER (LANG(?název)="cs")
      BIND(CONCAT(STR(?název), " (", STR(?code), ")") AS ?labelPředmětu)

      ?pracoviště skos:broader+ ?fakulta .
      ?fakulta skos:broader <https://cu.opendata.cz/resource/prvek-organizační-struktury/101> ;
         skos:notation ?fakultaID .
      BIND(IRI(CONCAT("https://linked.opendata.cz/ontology/knowledge-graph-browser/uk/názvy-předmětů-", STR(?fakultaID))) AS ?pFakulta)
    }
    GROUP BY ?node ?pFakulta
  }

    {
    SELECT ?node (COUNT(DISTINCT(?pracoviště)) AS ?početPracovišť) (COUNT(DISTINCT(?předmět)) AS ?početPředmětů)
    WHERE {
      ?předmět schema:provider ?pracoviště ;
        schema:keywords ?node .
    }
  }

}""" .

